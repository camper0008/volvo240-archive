        var maxH = screen.availHeight;

        if (maxW > 1024) {
          var newW = 1024;
        }
        else {
          var newW = maxW;
        }
        if (maxH > 768) {
          var newH = 768;
        }
        else {
          var newH = maxH;
        }
        window.resizeTo(newW, newH);
