<script Language="JavaScript" Type="text/javascript">
<!--
function FrontPage_Form1_Validator(theForm)
{

  if (theForm.txtSkadefri.value == "")
  {
    alert("Skriv en værdi for feltet \"Antal skadefri år\".");
    theForm.txtSkadefri.focus();
    return (false);
  }

  if (theForm.txtSkadefri.value.length < 1)
  {
    alert("Skriv mindst 1 tegn i feltet \"Antal skadefri år\".");
    theForm.txtSkadefri.focus();
    return (false);
  }

  var checkOK = "0123456789-";
  var checkStr = theForm.txtSkadefri.value;
  var allValid = true;
  var validGroups = true;
  var decPoints = 0;
  var allNum = "";
  for (i = 0;  i < checkStr.length;  i++)
  {
    ch = checkStr.charAt(i);
    for (j = 0;  j < checkOK.length;  j++)
      if (ch == checkOK.charAt(j))
        break;
    if (j == checkOK.length)
    {
      allValid = false;
      break;
    }
    allNum += ch;
  }
  if (!allValid)
  {
    alert("Skriv kun tal tegn i feltet \"Antal skadefri år\".");
    theForm.txtSkadefri.focus();
    return (false);
  }

  var chkVal = allNum;
  var prsVal = parseInt(allNum);
  if (chkVal != "" && !(prsVal >= 0))
  {
    alert("Skriv en værdi større end eller lig med \"0\" i feltet \"Antal skadefri år\".");
    theForm.txtSkadefri.focus();
    return (false);
  }

  if (theForm.txtCompagny.value == "")
  {
    alert("Skriv en værdi for feltet \"Forsikringselskabets navn\".");
    theForm.txtCompagny.focus();
    return (false);
  }

  if (theForm.txtCompagny.value.length < 1)
  {
    alert("Skriv mindst 1 tegn i feltet \"Forsikringselskabets navn\".");
    theForm.txtCompagny.focus();
    return (false);
  }

  if (theForm.txtSelvrisiko.value == "")
  {
    alert("Skriv en værdi for feltet \"Selvrisiko\".");
    theForm.txtSelvrisiko.focus();
    return (false);
  }

  if (theForm.txtSelvrisiko.value.length < 1)
  {
    alert("Skriv mindst 1 tegn i feltet \"Selvrisiko\".");
    theForm.txtSelvrisiko.focus();
    return (false);
  }

  var checkOK = "0123456789-";
  var checkStr = theForm.txtSelvrisiko.value;
  var allValid = true;
  var validGroups = true;
  var decPoints = 0;
  var allNum = "";
  for (i = 0;  i < checkStr.length;  i++)
  {
    ch = checkStr.charAt(i);
    for (j = 0;  j < checkOK.length;  j++)
      if (ch == checkOK.charAt(j))
        break;
    if (j == checkOK.length)
    {
      allValid = false;
      break;
    }
    allNum += ch;
  }
  if (!allValid)
  {
    alert("Skriv kun tal tegn i feltet \"Selvrisiko\".");
    theForm.txtSelvrisiko.focus();
    return (false);
  }

  var chkVal = allNum;
  var prsVal = parseInt(allNum);
  if (chkVal != "" && !(prsVal >= 0))
  {
    alert("Skriv en værdi større end eller lig med \"0\" i feltet \"Selvrisiko\".");
    theForm.txtSelvrisiko.focus();
    return (false);
  }

  if (theForm.txtCostYear.value == "")
  {
    alert("Skriv en værdi for feltet \"txtCostYear\".");
    theForm.txtCostYear.focus();
    return (false);
  }

  if (theForm.txtCostYear.value.length < 1)
  {
    alert("Skriv mindst 1 tegn i feltet \"txtCostYear\".");
    theForm.txtCostYear.focus();
    return (false);
  }

  var checkOK = "0123456789-";
  var checkStr = theForm.txtCostYear.value;
  var allValid = true;
  var validGroups = true;
  var decPoints = 0;
  var allNum = "";
  for (i = 0;  i < checkStr.length;  i++)
  {
    ch = checkStr.charAt(i);
    for (j = 0;  j < checkOK.length;  j++)
      if (ch == checkOK.charAt(j))
        break;
    if (j == checkOK.length)
    {
      allValid = false;
      break;
    }
    allNum += ch;
  }
  if (!allValid)
  {
    alert("Skriv kun tal tegn i feltet \"txtCostYear\".");
    theForm.txtCostYear.focus();
    return (false);
  }

  var chkVal = allNum;
  var prsVal = parseInt(allNum);
  if (chkVal != "" && !(prsVal >= 0))
  {
    alert("Skriv en værdi større end eller lig med \"0\" i feltet \"txtCostYear\".");
    theForm.txtCostYear.focus();
    return (false);
  }
  return (true);
}
